{
  "name": "trusted-reward-system-server",
  "version": "1.0.0",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --trace-warnings --watch src server.ts",
    "build": "tsc",
    "start": "node ./dist/server.js",
    "test": "jest --watchAll --runInBand --detectOpenHandles",
    "test:db": "ts-node-dev test/testdb.ts",
    "test:rpc": "ts-node-dev test/testrpc.ts",
    "migrate": "ts-node ./src/migrator.ts",
    "db:migrate:undo": "yarn sequelize-cli db:migrate:undo",
    "db:seed:generate": "yarn sequelize-cli db:seed:generate",
    "db:seed": "yarn sequelize-cli db:seed:all",
    "db:seed:undo": "yarn sequelize-cli db:seed:undo:all"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "pg": "^8.11.3",
    "sequelize": "^6.35.1",
    "umzug": "^3.4.0"
  }
}
